{
    "name": "leek/filament-dicebear",
    "description": "DiceBear avatar provider for Filament panels with 31 styles, caching, and per-model customization.",
    "keywords": [
        "leek",
        "laravel",
        "filament",
        "filamentphp",
        "avatar",
        "dicebear",
        "svg"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Jones",
            "email": "leeked@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/leek/filament-dicebear/issues",
        "source": "https://github.com/leek/filament-dicebear"
    },
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0 || ^4.0 || ^5.0",
        "spatie/laravel-package-tools": "^1.15"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "pestphp/pest": "^2.0 || ^3.0 || ^4.0",
        "pestphp/pest-plugin-laravel": "^2.0 || ^3.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Leek\\FilamentDiceBear\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leek\\FilamentDiceBear\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leek\\FilamentDiceBear\\DiceBearServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
